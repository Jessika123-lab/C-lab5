using System;

namespace Lab5_завд1
{
    class Program
    {
        static void Main(string[] args)
        {
            //завдання 1 лаб 5
            Console.Write("n=");
            int n = int.Parse(Console.ReadLine());
            double s = 0;
            s += (Math.Sqrt(2) + Math.Sqrt(4) + Math.Sqrt(2 * n)) + (Math.Pow(3, 3) + Math.Pow(5, 3) + Math.Pow(2 * n + 1, 3));
            Console.WriteLine("s= " + s);
        }
    }
}
